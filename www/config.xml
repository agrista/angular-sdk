<?xml version='1.0' encoding='utf-8'?>
<widget id="com.agrista.framework" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AgristaCore</name>
    <description>The mobile application for Agrista Enterprise</description>
    <author email="developer@agrista.com" href="http://agrista.com">
        Agrista
    </author>
    <access origin="*" />
    <log level="DEBUG" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="keepRunning" value="true" />
    <preference name="BackupWebStorage" value="none" />
</widget>
